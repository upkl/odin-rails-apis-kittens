<h1><%= @kitten.name %></h1>
<dl>
  <dt>Name</dt>
  <dd><%= @kitten.name %></dd>
  <dt>Age</dt>
  <dd><%= @kitten.age %></dd>
  <dt>Cuteness</dt>
  <dd><%= @kitten.cuteness %></dd>
  <dt>Softness</dt>
  <dd><%= @kitten.softness %></dd>
</dl>

<p><%= link_to 'Edit', edit_kitten_path(@kitten) %> <%= link_to 'Destroy', @kitten, data: { turbo_method: :delete } %></p>
<p><%= link_to 'New kitten', new_kitten_path %></p>
<p><%= link_to 'Back to index', kittens_path %></p>
